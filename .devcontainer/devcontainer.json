{
    "name": "Pyspark Dev Environment",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance"
            ]
        }
    },
    "remoteUser": "root",
    "mounts": [
        "target=/tmp:exec,type=tmpfs,consistency=cached"
    ]
}